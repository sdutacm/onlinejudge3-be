{
  "name": "onlinejudge3-be",
  "version": "3.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "midway-bin build -c -p tsconfig.prod.json && gulp dist:postprocess",
    "ci": "npm run cov",
    "clean": "midway-bin clean",
    "cov": "midway-bin cov --ts",
    "cov-serve": "static-server coverage/lcov-report -p 7087",
    "debug": "cross-env NODE_ENV=development midway-bin debug --ts",
    "dev": "cross-env NODE_ENV=development midway-bin dev --ts",
    "devtest": "cross-env TS_NODE_PROJECT=test/tsconfig.json NODE_ENV=test midway-bin test --ts",
    "lint": "eslint --fix {src,test}/**/*.ts",
    "start": "egg-scripts start --daemon --title=egg-server-onlinejudge3-be --framework=midway --ts",
    "start-build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "stop": "egg-scripts stop --title=egg-server-onlinejudge3-be",
    "test": "npm run lint && npm run devtest"
  },
  "dependencies": {
    "ajv": "^6.12.0",
    "chalk": "^4.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.10.0",
    "egg-sequelize": "^5.2.1",
    "md5": "^2.2.1",
    "midway": "^1.0.0",
    "mysql2": "^2.1.0",
    "sequelize-typescript": "^1.1.0",
    "sleep-promise": "^8.0.1",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/json-schema": "^7.0.4",
    "@types/md5": "^2.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.18",
    "@types/redis": "^2.8.18",
    "@types/traverse": "^0.6.32",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.6.0",
    "gulp": "^4.0.2",
    "midway-bin": "1",
    "midway-mock": "1",
    "mocha": "^6.2.0",
    "static-server": "^2.2.1",
    "through2": "^3.0.1",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.8.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "ci": {
    "version": "10, 12"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-bin-build": {
    "include": [
      "app/public"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "bLue",
  "license": ""
}
